{
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:3000/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:3000/ 'cypress run'",
    "typecheck": "nuxi typecheck",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.html,.css,.scss --max-warnings 0 --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.html,.css,.scss --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@cypress/vue": "^3.1.1",
    "@headlessui/vue": "^1.6.0",
    "@iconify/vue": "^3.2.1",
    "@nuxt/test-utils-edge": "^3.0.0-rc.1-27516498.2bfd749",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/color-mode": "^3.0.2",
    "@nuxtjs/eslint-config-typescript": "^9.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@vueuse/core": "^8.3.1",
    "@vueuse/nuxt": "^8.3.1",
    "c8": "^7.11.2",
    "cypress": "^9.6.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.1",
    "nuxt": "3.0.0-rc.1",
    "nuxt-windicss": "^2.3.1",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "sass": "^1.51.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.6.4",
    "vitest": "^0.10.0",
    "vue-tsc": "^0.34.11"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.1.8",
    "pinia": "^2.0.13"
  },
  "lint-staged": {
    "**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts,html,css,scss}": [
      "npm run lint"
    ]
  }
}
